{
  "name": "@aresrpg/aresrpg-world",
  "version": "1.0.0",
  "description": "Aresrpg world loading module",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "test": "node test/index.test.js",
    "coverage": "c8 --check-coverage node test/index.test.js",
    "lint": "prettier-eslint $PWD/\"**/*.js\" --print-width 80 --list-different && eslint --color .",
    "format": "prettier-eslint $PWD/\"**/*.js\" --print-width 80 --write",
    "example": "node example/index.js",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AresRPG/aresrpg-world.git"
  },
  "keywords": [
    "aresrpg",
    "build"
  ],
  "author": "AresRPG",
  "license": "UNLICENSE",
  "homepage": "https://aresrpg.io",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/AresRPG/aresrpg-world/issues"
  },
  "babel": {
    "parserOpts": {
      "allowAwaitOutsideFunction": true
    }
  },
  "dependencies": {
    "spiralloop": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@hydre/doubt": "^7.1.0",
    "@sidy/eslint-config-sidy": "^1.4.15",
    "babel-eslint": "^11.0.0-beta.2",
    "c8": "^7.3.0",
    "eslint": "^7.9.0",
    "eslint-plugin-unicorn": "^22.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "tap-spec-emoji": "^6.1.2"
  }
